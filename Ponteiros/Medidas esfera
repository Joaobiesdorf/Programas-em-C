#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

void param_esfera(float r, float *area, float *volume){
     *area = 4 * 3.14 * (r*r);
    *volume = 0.75 * 3.14 * (r*r*r);

    printf("%.2f ", *area);
    printf("%.2f", *volume);

}
int main() {
    float r, *area, *volume;

    scanf("%f", &r);

    param_esfera(r, &area, &volume);

    return 0;
}
