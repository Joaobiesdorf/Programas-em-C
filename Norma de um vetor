#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

typedef struct{
    float x;
    float y;
    float norm;
}Vetor_t;

float calculaNorma(float x, float y){
    float norm;

   norm = sqrt(x*x + y*y);
   printf("Norma = %.1f\n", norm);
    return 0;

}

int main(){
int qnt, i;

  scanf("%d", &qnt);
  Vetor_t Vec[qnt];

  for(i=0; i<qnt; i++){
    scanf("%f", &Vec[i].x);
    scanf("%f", &Vec[i].y);

}
         for(i=0; i<qnt; i++){
calculaNorma(Vec[i].x, Vec[i].y);
         }

  return 0;
}
